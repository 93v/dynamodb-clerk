{
  "name": "dynamodb-clerk",
  "version": "0.0.0-development",
  "description": "Backup and Restore DynamoDB Tables",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "NODE_OPTIONS='--max-old-space-size=8192' ts-node ./src/index.ts",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "dynamodb-clerk": "./dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/93v/dynamodb-clerk.git"
  },
  "keywords": [
    "DynamoDB",
    "DynamoDB Clerk",
    "DynamoDB Dump",
    "Backup",
    "Restore"
  ],
  "author": {
    "name": "93v",
    "email": "93v@protonmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/93v/dynamodb-clerk/issues"
  },
  "homepage": "https://github.com/93v/dynamodb-clerk#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/async-retry": "^1.4.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/filesize": "^4.2.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^24.0.18",
    "@types/listr": "^0.14.2",
    "@types/node": "^12.7.12",
    "@types/ora": "^3.2.0",
    "@types/properties-reader": "0.0.1",
    "@types/rimraf": "^2.0.2",
    "@types/tar": "^4.0.3",
    "@types/yargs": "^13.0.3",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "sazerac": "^1.1.0",
    "semantic-release": "^15.13.24",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.6.4"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "async-retry": "^1.2.3",
    "aws-sdk": "^2.548.0",
    "filesize": "^5.0.3",
    "inquirer": "^7.0.0",
    "listr": "^0.14.3",
    "ora": "^4.0.2",
    "properties-reader": "^0.3.1",
    "rimraf": "^3.0.0",
    "tar": "^5.0.5",
    "ts-optchain": "^0.1.8",
    "yargs": "^14.2.0"
  }
}
