{
  "name": "dynamodb-clerk",
  "version": "0.0.0-development",
  "description": "Backup and Restore DynamoDB Tables",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "ts-node ./src/index.ts",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "dynamodb-clerk": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/93v/dynamodb-clerk.git"
  },
  "keywords": [
    "DynamoDB",
    "DynamoDB Clerk",
    "DynamoDB Dump",
    "Backup",
    "Restore"
  ],
  "author": {
    "name": "93v",
    "email": "93v@protonmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/93v/dynamodb-clerk/issues"
  },
  "homepage": "https://github.com/93v/dynamodb-clerk#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint && npm test"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@types/async-retry": "^1.4.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^26.0.0",
    "@types/listr": "^0.14.2",
    "@types/node": "^14.0.13",
    "@types/ora": "^3.2.0",
    "@types/properties-reader": "0.0.2",
    "@types/rimraf": "^3.0.0",
    "@types/tar": "^4.0.3",
    "@types/yargs": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "eslint": "^7.3.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "sazerac": "^2.0.0",
    "semantic-release": "^17.0.8",
    "ts-jest": "^26.1.0",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "async-retry": "^1.3.1",
    "aws-sdk": "^2.701.0",
    "inquirer": "^7.2.0",
    "listr": "^0.14.3",
    "ora": "^4.0.4",
    "pretty-bytes": "^5.3.0",
    "properties-reader": "^2.0.0",
    "rimraf": "^3.0.2",
    "tar": "^6.0.2",
    "yargs": "^15.3.1"
  }
}
