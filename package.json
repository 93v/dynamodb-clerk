{
  "name": "dynamodb-clerk",
  "version": "0.0.0-development",
  "description": "Backup and Restore DynamoDB Tables",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint './src/**/*.{js,ts,tsx}'",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watchAll",
    "build": "tsc",
    "watch": "tsc -w",
    "start": "ts-node ./src/index.ts",
    "semantic-release": "semantic-release"
  },
  "bin": {
    "dynamodb-clerk": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/93v/dynamodb-clerk.git"
  },
  "keywords": [
    "DynamoDB",
    "DynamoDB Clerk",
    "DynamoDB Dump",
    "Backup",
    "Restore"
  ],
  "author": {
    "name": "93v",
    "email": "93v@protonmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/93v/dynamodb-clerk/issues"
  },
  "homepage": "https://github.com/93v/dynamodb-clerk#readme",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run lint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/async-retry": "^1.4.2",
    "@types/aws-sdk": "^2.7.0",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.16",
    "@types/listr": "^0.14.2",
    "@types/node": "^14.14.10",
    "@types/ora": "^3.2.0",
    "@types/properties-reader": "2.1.0",
    "@types/yargs": "^15.0.11",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.2.0",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "sazerac": "^2.0.0",
    "semantic-release": "^17.3.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@supercharge/promise-pool": "^1.6.0",
